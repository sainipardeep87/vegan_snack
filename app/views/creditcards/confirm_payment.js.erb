<% if @cart.is_full? %>

    $(".full-page").css("display", "none");
    $("#ship_area").empty('');
    $("#ship_area").append($("#existing_address").html());
    $('.snack-queue-page').remove();
    $(".address-section").css('display','block');
   $('html').height($(window).height());

<% else %>

$("#new_sub_incomplete_cart div").css("display", "block");

$("#queue_title").ScrollTo({
            duration: 1000
});

setTimeout(function(){
    $("#new_sub_incomplete_cart div").fadeOut();
}, 1800);

//alert("Cart is not complete yet.");

<% end %>