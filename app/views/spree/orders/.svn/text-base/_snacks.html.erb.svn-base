<% if @sub_items.present? %>  <% @sub_items.each do |key,value| %>    <% @dyna_class = 'first-delivery' if key == 0 %>    <% @dyna_class = 'second-delivery' if key == 1 %>    <% @dyna_class = 'third-delivery' if key == 2 %>    <div class="<%= @dyna_class %> subscription-row">      <div class="queue-tile  no-margin">            <span class="month-tile">                <div class='hide-mobile-ship'>next order Ships by</div>                <%= @delivery_dates[key].strftime("%B %d, %Y") %>            </span>      </div>      <div class="resized-snacks">        <% value.each do |line_item| %>          <% line_item.quantity.times do %>            <% variant = line_item.variant %>                <div class="queue-tile">                  <div class="queue-image-title">                    <span> <%= image_tag(variant.images.first.attachment.url(:small), class: 'img-thumbnail') %> </span>                    <span> <%= line_item.name %> </span>                  </div>                </div>            <% end %>            <% end %> <!-- End of the value.each loop -->      </div>    </div>  <% end %>   <% else %>  <div class="alert alert-info">No items found!</div><% end %>