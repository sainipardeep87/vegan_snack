.container{
    width: 100% !important;
}

.about-us-container, .save-planet-container{
    padding: 0% !important;
    margin: 0% !important;
}

.about-us-background{
    background: url('/assets/vegan-about/about_us_bg_1_low.jpg');
    background-size: 100% 100%;
}

.top_banner {
    background-repeat: no-repeat;
    background-size: 100%;
    width: 100%;
    height: 0;
    margin-top: 5%;
}

/* Above lines are css of header part only */
.bg-image {
    background-image: url("/assets/vegan-about/about-us-bg-1.jpg");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    margin-top: 51px;
    min-height: 577px; /* this will be window height - header height-20gutter width; */
}


.save-planet-image{
    background-image: url("/assets/vegan-about/about_us_bg_2_low.jpg");
    background-size: 100% 100%;
}

.bg-image h1 {
    color: #FF6501;
    font-size: 42px !important;
    font-weight: bold;
    text-align: center;
    text-transform: uppercase;
}

.about-title {
    color: #F4700B;
    /*font-family: 'bold-font';*/
    font-family: Arial, Helvetica, sans-serif;
    font-weight: bold;
    margin: 8% 0% 0% 0%;
    text-align: center;
    text-transform: uppercase;
}

.about-title > h1 {
    font-size: 3.0em;
    font-weight: bold;
} 

.top-buffer {
    margin-top: 20px;
    margin-left:13%;
}

.about_us_wrapper {
    padding: 0 0 0 42px;
    margin-top: -1%;
    font-size: 11pt;    
}

.about-us-content {
    background-color: rgba(255, 255, 255, 0.94);
    border-radius: 9px;
    color: #000000;
    padding-bottom: 2%;
    padding-right: 2%;
    padding-top: 2%;
    /*font-family: 'regular-font';*/
    font-family: Arial, Helvetica, sans-serif;
}

.save-planet-content{
    background-color: rgba(255, 255, 255, 0.698);
    border-radius: 9px;
    color: #000000;
    padding-bottom: 2%;
    padding-right: 2%;
    padding-top: 2%;
    /*font-family: 'regular-font';*/
    font-family: Arial, Helvetica, sans-serif;
}

.save-planet-title {
    color: #FFFFFF;
    /*font-family: bold-font;*/
    font-family: Arial, Helvetica, sans-serif;
    font-weight: bold;
    font-size: 3em !important;
    margin: 6% 0 3%;
    text-align: center;
    text-transform: uppercase;
}

.save-planet-title h1{
    font-size: 1.1em;
    font-weight: bold;
}

.chart-image{ height: 305px; }


.about-us-vision{
    color: #D4511B;
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 1%; 
}

.name-meaning-content {
    margin-top: 1px;
    font-size: 11pt;
}

.chef-image {
    height: 350px;
    width: 122%;
}

div.about-us-content > div { line-height: 170%; }

